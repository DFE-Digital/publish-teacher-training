<%= render PageTitle.new(title: "support.rollover.index") %>

<h1 class="govuk-heading-l"><%= t("components.page_titles.support.rollover.index") %></h1>

<ul class="govuk-list govuk-list--spaced">

  <li>
    <%= govuk_link_to(
          "#{current_recruitment_cycle_period_text} - current",
          support_recruitment_cycle_providers_path(Settings.current_recruitment_cycle_year),
        ) %>
  </li>

  <% RecruitmentCycle.next_editable_cycles.each do |recruitment_cycle| %>
    <li>
        <%= govuk_link_to(
        recruitment_cycle.year_range,
        support_recruitment_cycle_providers_path(recruitment_cycle.year),
      ) %>
    </li>
  <% end %>
</ul>

<table class="govuk-table">
  <thead class="govuk-table__head">
    <tr class="govuk-table__row">
      <th scope="col" class="govuk-table__header govuk-!-width-one-quarter"><%= t("table_heading.id") %></th>
      <th scope="col" class="govuk-table__header govuk-!-width-one-quarter"><%= t("table_heading.ease_of_use") %></th>
      <th scope="col" class="govuk-table__header govuk-!-width-one-quarter"><%= t("table_heading.experience") %></th>
      <th scope="col" class="govuk-table__header govuk-!-width-one-quarter"><%= t("table_heading.created_at") %></th>
    </tr>
  </thead>
  <tbody class="govuk-table__body">
    <% feedbacks.each do |feedback| %>
      <tr class="govuk-table__row">
        <td class="govuk-table__cell">
          <%= feedback.id %>
        </td>
        <td class="govuk-table__cell">
          <%= feedback.ease_of_use.humanize %>
        </td>
        <td class="govuk-table__cell">
          <%= feedback.experience %>
        </td>
        <td class="govuk-table__cell">
          <%= l(feedback.created_at, format: :last_event) %>
        </td>
      </tr>
    <% end %>
  </table>

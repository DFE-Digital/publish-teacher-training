<table class="govuk-table app-table--vertical-align-middle app-table--courses">
  <thead class="govuk-table__head">
    <tr class="govuk-table__row">
      <th class="govuk-table__header"><%= t(".course") %></th>
      <th class="govuk-table__header"><%= t(".status") %></th>
      <th class="govuk-table__header"><%= t(".view_course") %></th>
      <th class="govuk-table__header"><%= t(".applications") %></th>
    </tr>
  </thead>
  <tbody class="govuk-table__body">
    <%= render partial: "publish/courses/course_table_row", collection: courses, as: :course %>
  </tbody>
</table>

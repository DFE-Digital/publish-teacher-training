<% provider = partnership.accredited_provider %>
<% body.with_row do |row| %>
  <% row.with_cell do %>
    <%= govuk_link_to(
      "#{provider.provider_name} (#{provider.provider_code})",
      details_publish_provider_recruitment_cycle_accredited_partnership_path(
        provider_code: @provider.provider_code,
        recruitment_cycle_year: @provider.recruitment_cycle_year,
        accredited_provider_code: provider.provider_code,
      ),
    ) %>
  <% end %>
<% end %>

<%= govuk_table do |table| %>
  <% table.with_head do |head| %>
    <% head.with_row do |row| %>
      <% row.with_cell(text: "Name") %>
    <% end %>
  <% end %>
  <% table.with_body do |body| %>
    <% partnerships.each do |partnership| %>
      <%= render partial: "publish/providers/accredited_partnerships/accredited_provider_table_row", locals: { partnership: partnership, body: body } %>
    <% end %>
  <% end %>
<% end %>

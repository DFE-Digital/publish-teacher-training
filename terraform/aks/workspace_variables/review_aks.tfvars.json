{
  "env_config": "review",
  "app_environment": "review",
  "key_vault_name": "s189t01-ptt-rv-kv",
  "key_vault_app_secret_name": "PUBLISH-APP-SECRETS-QA",
  "key_vault_infra_secret_name": "BAT-INFRA-SECRETS-QA",
  "cluster": "test",
  "namespace": "bat-qa",
  "deploy_azure_backing_services": false,
  "db_sslmode": "prefer",
  "resource_group_name": "s189t01-ptt-rv-rg",
  "postgres_create_servicename_db": true,
  "postgres_extensions": "PGCRYPTO,BTREE_GIST,CITEXT,PG_TRGM",
  "postgres_version": "13",
  "main_app": {
    "main": {
      "startup_command": ["/bin/sh", "-c", "bundle exec rails db:migrate && bundle exec rails server -b 0.0.0.0"],
      "replicas": 1,
      "memory_max": "1Gi"
    }
  },
  "worker_apps": {
    "worker": {
      "startup_command": ["/bin/sh", "-c", "bundle exec sidekiq -C config/sidekiq.yml"],
      "replicas": 1,
      "memory_max": "1Gi"
    }
  },
  "paas_web_app_stopped": false,
  "paas_worker_app_stopped": false,
  "enable_find": true,
  "enable_monitoring": false
}
